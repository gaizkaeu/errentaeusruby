{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "constitucion": {
            "type": "string",
            "enum": ["sociedad", "autonomo"]
        },
        "trabajadores": {
            "type": "integer",
            "minimum": 1
        },
        "impuestos_especiales": {
            "type": "integer",
            "minimum": 0,
            "maximum": 1
        }
    },
    "required": ["constitucion", "trabajadores"],
    "additionalProperties": false
  }
  